{
  "name": "gb-player",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "^25.0.2",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.18",
    "@types/react-test-renderer": "^16.0.1",
    "husky": "^0.14.3",
    "jest-expo": "25.0.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react-native-scripts": "1.11.1",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "lint":
      "node node_modules/tslint/bin/tslint -c tslint.json './src/**/*.{ts,tsx}'",
    "test": "node node_modules/jest/bin/jest.js",
    "format":
      "node node_modules/prettier/bin-prettier.js --write .babelrc .prettierrc .watchmanconfig App.js '*.json' 'src/**/*.{ts,tsx,js}'",
    "precommit": "yarn format && yarn lint && yarn test",
    "prepush": "yarn format && yarn lint && yarn test"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "expo": "^25.0.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "tslib": "^1.9.0"
  }
}
