{
  "name": "gb-player",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "^25.0.2",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.18",
    "@types/react-navigation": "^1.2.3",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.1",
    "exp": "^50.0.1",
    "husky": "^0.14.3",
    "jest-expo": "25.0.0",
    "jest-fetch-mock": "^1.5.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "react-native-scripts": "1.11.1",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "tsc -p tsconfig.json && jest --config jest.config.js",
    "lint-staged": "lint-staged",
    "exp-login":
      "exp login --non-interactive -u $EXP_USERNAME -p $EXP_PASSWORD",
    "exp-publish": "exp publish",
    "exp-publish-test": "exp publish --release-channel test",
    "clean": "rm -rf ./node_modules ./expo ./jest ./yarn-error.log",
    "lint": "tslint -c tslint.json ./src/**/*.{ts,tsx}",
    "format":
      "prettier --write ./**/*.{ts,tsx,js,jsx,json,css,md,babelrc,prettierrc,watchmanconfig}",
    "ci-build": "yarn lint && yarn test",
    "precommit": "yarn lint-staged && yarn test"
  },
  "dependencies": {
    "expo": "^25.0.0",
    "fast-xml-parser": "^3.3.6",
    "query-string": "^6.0.0",
    "react": "16.2.0",
    "react-native": "0.52.2",
    "react-native-button": "^2.3.0",
    "react-navigation": "^1.5.8",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "tslib": "^1.9.0"
  }
}
