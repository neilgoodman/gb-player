{
  "name": "gb-player",
  "version": "1.0.0-alpha",
  "description": "A video player app for viewing content on giantbomb.com.",
  "main": "index.html",
  "user-agent": "GB Player %ver (node-webkit %nwver) %osinfo",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Neil Goodman",
  "license": "MIT",
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "bootstrap": {
      "exports": null,
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "devDependencies": {
    "browserify": "^6.3.2",
    "browserify-shim": "^3.8.0",
    "express": "^4.10.2",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.2.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-less": "^0.12.0",
    "grunt-contrib-uglify": "^0.6.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-exec": "^0.4.6",
    "grunt-jsdoc": "^0.5.7",
    "grunt-node-webkit-builder": "^0.3.0",
    "hbsfy": "^2.2.0"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "bootstrap": "^3.3.1",
    "handlebars": "^2.0.0",
    "jquery": "^2.1.1",
    "underscore": "^1.7.0",
    "underscore.string": "^2.4.0"
  }
}
